{
  "name": "frontline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev":
      "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "prod":
      "cross-env NODE_ENV=production rimraf ./dist/* -rf && webpack -p --config webpack.config.prod.js",
    "postinstall": "yarn prod"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.{js,jsx,mjs}"],
    "setupFiles": ["<rootDir>/config/polyfills.js"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)":
        "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.37",
    "@babel/polyfill": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "@babel/preset-react": "^7.0.0-beta.37",
    "@babel/preset-stage-2": "^7.0.0-beta.37",
    "@babel/preset-stage-3": "^7.0.0-beta.37",
    "@babel/register": "^7.0.0-beta.37",
    "autoprefixer": "6.3.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.1.3",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "css-mqpacker": "^6.0.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.4",
    "postcss": "^6.0.16",
    "postcss-clean": "^1.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-inline-media": "^0.0.2",
    "postcss-loader": "^2.0.9",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "react-hot-loader": "^3.1.3",
    "react-router-redux": "^4.0.8",
    "style-loader": "^0.19.1",
    "svg-inline-loader": "^0.8.0",
    "tailwindcss": "^0.4.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "firebase": "^4.10.1",
    "formik": "^0.11.10",
    "moment": "^2.20.1",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.0",
    "querystringify": "^1.0.0",
    "react": "^16.2.0",
    "react-autosuggest": "^9.3.4",
    "react-dom": "^16.2.0",
    "react-paystack": "^1.0.1",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.0.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-toastify": "^3.3.4",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.2.0",
    "sweetalert": "^2.1.0",
    "uppy": "^0.23.0",
    "valib": "^2.0.0",
    "validator": "^9.4.1"
  }
}
